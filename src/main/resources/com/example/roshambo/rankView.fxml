<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="1000" prefWidth="1000" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.roshambo.Rankliste">
   <children>
      <Pane prefHeight="1000.0" prefWidth="1000.0">
         <children>
            <ImageView fitHeight="1000.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@landscape/fight.jpg" />
               </image>
            </ImageView>
            <TableView fx:id="rankTable" layoutX="60.0" layoutY="187.0" opacity="0.62" prefHeight="784.0" prefWidth="635.0">
              <columns>
                <TableColumn fx:id="colWins" prefWidth="196.0" text="Wins" />
                <TableColumn fx:id="colPlayer" minWidth="0.0" prefWidth="277.0" text="Player" />
              </columns>
            </TableView>
            <Label alignment="CENTER" layoutX="60.0" layoutY="33.0" prefHeight="110.0" prefWidth="872.0" text="RANKLIST">
               <font>
                  <Font size="48.0" />
               </font>
            </Label>
            <Button fx:id="playButton" onAction="#onPlayClicked" layoutX="748.0" layoutY="212.0" mnemonicParsing="false" prefHeight="53.0" prefWidth="197.0" text="Spielen" />
            <Button fx:id="exitButton" onAction="#onExitClicked" layoutX="748.0" layoutY="286.0" mnemonicParsing="false" prefHeight="53.0" prefWidth="197.0" text="Beenden"/>
         </children>
      </Pane>
   </children>
</AnchorPane>
