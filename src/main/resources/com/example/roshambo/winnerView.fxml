<AnchorPane prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.roshambo.WinnerController">
   <children>
       <Pane fx:id="ersteSeite" prefHeight="1000.0" prefWidth="1000.0">
           <children>
               <ImageView fitHeight="1000.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
                   <image>
                       <Image url="@landscape/fight.jpg" />
                   </image>
               </ImageView>
            <ImageView fitHeight="349.0" fitWidth="514.0" layoutX="331.0" layoutY="169.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@character/crown.png" />
               </image>
            </ImageView>
               <Label alignment="CENTER" prefHeight="200.0" prefWidth="1000.0" text="Ro-Sham-Bo">
                   <font>
                       <Font name="Darumadrop One Regular" size="48.0" />
                   </font>
               </Label>
               <Label fx:id="winnerText" alignment="CENTER" layoutX="166.0" layoutY="508.0" prefHeight="70.0" prefWidth="664.0" text="&quot;Winner Name&quot;">
                   <font>
                       <Font name="Darumadrop One Regular" size="48.0" />
                   </font>
               </Label>
               <Button fx:id="rangliste" onAction="#zeigeRangliste" layoutX="286.0" layoutY="648.0" mnemonicParsing="false" prefHeight="57.0" prefWidth="142.0" text="Rangliste">
                   <font>
                       <Font name="Darumadrop One Regular" size="24.0" />
                   </font>
               </Button>
               <Button fx:id="again" layoutX="538.0" layoutY="648.0" mnemonicParsing="false" onAction="#again" prefHeight="57.0" prefWidth="142.0" text="Nochmal">
